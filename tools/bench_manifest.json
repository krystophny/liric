{
  "schema_version": 2,
  "description": "Canonical benchmark matrix manifest for bench_matrix.",
  "axes": {
    "modes": ["isel", "copy_patch", "llvm"],
    "policies": ["direct", "ir"]
  },
  "lanes": {
    "api_exe": {
      "kind": "primitive",
      "baseline": "lfortran_llvm",
      "source": "bench_api"
    },
    "api_jit": {
      "kind": "primitive",
      "baseline": "lfortran_llvm",
      "source": "bench_api"
    },
    "ll_jit": {
      "kind": "primitive",
      "baseline": "llvm",
      "source": "bench_corpus_compare"
    },
    "ll_llvm": {
      "kind": "primitive",
      "baseline": "llvm",
      "source": "bench_corpus_compare"
    },
    "api_e2e": {
      "kind": "derived",
      "from": ["api_exe", "api_jit"],
      "compat_alias": true
    },
    "ll_e2e": {
      "kind": "derived",
      "from": ["ll_llvm", "ll_jit"]
    },
    "ir_file": {
      "kind": "derived",
      "from": ["ll_llvm", "ll_jit"],
      "compat_alias": true
    }
  },
  "policy": {
    "unsupported": "hard_fail",
    "missing_cell": "hard_fail",
    "empty_dataset": "hard_fail"
  }
}
