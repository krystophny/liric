#!/usr/bin/env bash
set -euo pipefail
case "${1:-}" in
  --version)
    echo "10.0.1"
    ;;
  --includedir)
    echo "/home/ert/code/lfortran-dev/liric/build_llvm_ci/ctest_work/llvm_backend_compat_policy_cases/llvm10_with_compat_on/include"
    ;;
  --cflags)
    echo ""
    ;;
  --ldflags)
    echo ""
    ;;
  --libs)
    echo ""
    ;;
  --system-libs)
    echo ""
    ;;
  *)
    echo "fake llvm-config: unsupported arg $1" >&2
    exit 91
    ;;
esac
